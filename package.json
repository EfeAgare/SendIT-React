{
  "name": "sendit",
  "version": "1.0.0",
  "description": "SendIT is a courier service that helps users deliver parcels to different destinations.",
  "main": "index.js",
  "scripts": {
    "test": "set NODE_ENV=test&&npm run migrate&& npm run seed && nyc mocha ./server/src/tests/ --compilers js:@babel/register --require @babel/polyfill --timeout 30000 --exit",
    "devstart": "nodemon server/src/index.js --exec babel-node",
    "build": "babel ./server/src -d ./server/dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "heroku-postbuild": "npm run build && npm run migrate && npm run seed",
    "migrate": "babel-node ./server/src/models/db.js",
    "seed": "babel-node ./server/src/models/seed.js",
    "react": "webpack-dev-server --mode development",
    "start": "node server/dist/index.js",
    "connect": "concurrently \"npm run devstart\" \"npm run react\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EfeAgare/SendIT.git"
  },
  "author": "Agare Efe Knowledge",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EfeAgare/SendIT/issues"
  },
  "homepage": "https://github.com/EfeAgare/SendIT#readme",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.0.0",
    "express-validator": "^5.3.0",
    "jsonwebtoken": "^8.4.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "lodash.times": "^4.3.2",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^4.7.0",
    "pg": "^7.6.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^0.85.0",
    "swagger-ui-express": "^4.0.1",
    "webpack-hot-middleware": "^2.24.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "coveralls": "^3.0.2",
    "css-loader": "^2.1.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-mocha": "^5.2.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.7",
    "nyc": "^13.3.0",
    "prop-types": "^15.6.2",
    "react-hot-loader": "^4.6.3",
    "style-loader": "^0.23.1",
    "supertest": "^3.3.0",
    "uuid": "^3.3.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.0",
